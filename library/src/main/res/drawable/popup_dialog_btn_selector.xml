<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/popup_dialog_corner" />
            <solid android:color="@color/popup_dialog_btn_pressed" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/popup_dialog_corner" />
            <solid android:color="@color/popup_dialog_btn_normal" />
        </shape>
    </item>
</selector>